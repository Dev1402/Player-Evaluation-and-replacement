    "df_com['Passes (total)'] = df_com[ 'high_passes']+ df_com['headed_passes'] + df_com['ground_passes']\n",
    "df_com['Average carries attacking third'] = df_com[ 'carry_distance_attacking_third']/df_com[ 'carries_attacking_third']\n",
    "\n",
    "#passing\n",
    "df_com['all pass'] = df_com['Passes attacking third(total)']+df_com['Passes (total)']+df_com['Passes box(total)'] \n",
    "df_com['Passing successful'] = df_com['high_passes_attacking_third_successful']+df_com['headed_passes_attacking_third_successful'] + df_com['ground_passes_attacking_third_successful']+ df_com['high_passes_box_successful']+ df_com['headed_passes_box_successful'] + df_com['ground_passes_box_successful']+ df_com['high_passes_successful']+ df_com['headed_passes_successful'] + df_com['ground_passes_successful']\n",
    "df_com['passing accuracy'] =df_com['Passing successful']/df_com['all pass']\n",
    "\n",
    "#defense\n",
    "df_com['Defensive duels %'] = (df_com[ 'defensive_ground_duels_successful']+df_com[ 'defensive_aerial_duels_successful'])/(df_com['defensive_ground_duels']+df_com['defensive_aerial_duels'])\n",
    "df_com['interceptions'] = df_com['ground_interceptions']+df_com['headed_interceptions']\n",
    "df_com['clearences'] = df_com['ground_clearances']+df_com['headed_clearances']"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 51,
   "metadata": {},
   "outputs": [],
   "source": [
    "# This will used to compare across both the leagues\n",
    "analyze_competition_attack = pd.DataFrame()\n",
    "analyze_competition_passing = pd.DataFrame()\n",
    "analyze_competition_defense = pd.DataFrame()\n",
    "analyze_competition_attack = df_com.filter(['team','Goal involvement','Passes attacking third(total)',  'crosses', 'ground_shots_box_perc','Average carries attacking third'], axis=1)\n",
    "analyze_competition_defense = df_com.filter(['team','interceptions','clearences','blocks','Defensive duels %'], axis=1)\n",
    "analyze_competition_passing = df_com.filter(['team','high pass ratio','passing accuracy','dribbles'], axis=1)\n"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# Age and Position wise analysis"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 52,
   "metadata": {},
   "outputs": [],
   "source": [
    "def age_group(x):\n",
    "    if (x>=18)&(x<24):\n",
    "        return 'low'\n",
    "    if (x>=24)&(x<28):\n",
    "        return 'medium'\n",
    "    if (x>=28):\n",
    "        return 'high'\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 53,
   "metadata": {},
   "outputs": [],
   "source": [
    "#creating age profile with respect to the position of the players\n",
    "df_raw['age group'] = df_raw['age'].apply(age_group)\n",
    "df_age = round(df_raw.groupby(['age group', 'position']).mean(),2).reset_index()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 54,
   "metadata": {},
   "outputs": [],
   "source": [
    "# attacking\n",
    "df_age['Goal involvement'] = df_age['goals']+df_age['assists']\n",
    "df_age['Passes attacking third(total)'] = df_age[ 'high_passes_attacking_third']+ df_age['headed_passes_attacking_third'] + df_age['ground_passes_attacking_third']\n",
    "df_age['Passes box(total)'] = df_age[ 'high_passes_box']+ df_age['headed_passes_box'] + df_age['ground_passes_box']\n",
    "df_age['Passes (total)'] = df_age[ 'high_passes']+ df_age['headed_passes'] + df_age['ground_passes']\n",
    "df_age['Average carries attacking third'] = df_age[ 'carry_distance_attacking_third']/df_age[ 'carries_attacking_third']\n",
    "\n",
    "#passing\n",
    "df_age['all pass'] = df_age['Passes attacking third(total)']+df_age['Passes (total)']+df_age['Passes box(total)'] \n",
    "df_age['Passing successful'] = df_age['high_passes_attacking_third_successful']+df_age['headed_passes_attacking_third_successful'] + df_age['ground_passes_attacking_third_successful']+ df_age['high_passes_box_successful']+ df_age['headed_passes_box_successful'] + df_age['ground_passes_box_successful']+ df_age['high_passes_successful']+ df_age['headed_passes_successful'] + df_age['ground_passes_successful']\n",
    "df_age['passing accuracy'] =df_age['Passing successful']/df_age['all pass']\n",
    "\n",
    "#defense\n",
    "df_age['Defensive duels %'] = (df_age[ 'defensive_ground_duels_successful']+df_age[ 'defensive_aerial_duels_successful'])/(df_age['defensive_ground_duels']+df_age['defensive_aerial_duels'])\n",
    "df_age['interceptions'] = df_age['ground_interceptions']+df_age['headed_interceptions']\n",
    "df_age['clearences'] = df_age['ground_clearances']+df_age['headed_clearances']\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 55,
   "metadata": {},
   "outputs": [],
   "source": [
    "# This will used to compare across age and position profile\n",
    "analyze_age_attack = pd.DataFrame()\n",
    "analyze_age_defense = pd.DataFrame()\n",
    "analyze_age_passing = pd.DataFrame()\n",

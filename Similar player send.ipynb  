    "df_all['Passes box(total)'] = df_all[ 'high_passes_box']+ df_all['headed_passes_box'] + df_all['ground_passes_box']\n",
    "df_all['Passes (total)'] = df_all[ 'high_passes']+ df_all['headed_passes'] + df_all['ground_passes']\n",
    "df_all['Average carries attacking third'] = df_all[ 'carry_distance_attacking_third']/df_all[ 'carries_attacking_third']\n",
    "\n",
    "#pasing group by player\n",
    "df_all['all pass'] = df_all['Passes attacking third(total)']+df_all['Passes (total)']+df_all['Passes box(total)'] \n",
    "df_all['Passing successful'] = df_all['high_passes_attacking_third_successful']+df_all['headed_passes_attacking_third_successful'] + df_all['ground_passes_attacking_third_successful']+ df_all['high_passes_box_successful']+ df_all['headed_passes_box_successful'] + df_all['ground_passes_box_successful']+ df_all['high_passes_successful']+ df_all['headed_passes_successful'] + df_all['ground_passes_successful']\n",
    "df_all['passing accuracy'] =df_all['Passing successful']/df_all['all pass']\n",
    "df_all['high pass ratio'] = (df_all['high_passes']+ df_all['high_passes_attacking_third'] + df_all['high_passes_box']/df_all['all pass'])\n",
    "\n",
    "#adefense group by player\n",
    "df_all['Defensive duels %'] = (df_all[ 'defensive_ground_duels_successful']+df_all[ 'defensive_aerial_duels_successful'])/(df_all[ 'defensive_ground_duels']+df_all[ 'defensive_aerial_duels'])\n",
    "df_all['interceptions'] = df_all['ground_interceptions']+df_all['headed_interceptions']\n",
    "df_all['clearences'] = df_all['ground_clearances']+df_all['headed_clearances']\n",
    "\n",

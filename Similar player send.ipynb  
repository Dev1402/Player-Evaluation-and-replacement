    "df_com['Goal involvement'] = df_com['goals']+df_com['assists']\n",
    "df_com['Passes attacking third(total)'] = df_com[ 'high_passes_attacking_third']+ df_com['headed_passes_attacking_third'] + df_com['ground_passes_attacking_third']\n",
